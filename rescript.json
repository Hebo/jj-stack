{
  "name": "jj-stack",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.mjs",
  "bs-dependencies": ["@rescript/core", "@rescript/react"],
  "bsc-flags": ["-open RescriptCore"],
  "gentypeconfig": {
    "module": "node16",
    "moduleResolution": "node16",
    "generatedFileExtension": ".gen.tsx"
  },
  "jsx": { "version": 4 }
}
